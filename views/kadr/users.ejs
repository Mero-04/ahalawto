<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>Işgärler</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="/admin/assets/img/favicon.png" rel="icon">
  <link href="/admin/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/admin/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/admin/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/admin/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/admin/assets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="/admin/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="/admin/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="/admin/assets/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/admin/assets/css/style.css" rel="stylesheet">

 </head>

<body>

  <!-- ======= Header ======= -->
  <%- include('../component/header-kadr') %>
  <!-- End Header -->

  <!-- ======= Sidebar ======= -->

  <%- include('../component/sidebar-kadr') %>

  <!-- End Sidebar-->

  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Işgärler sahypasy</h1>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <div class="card-header d-flex justify-content-between align-items-center">
                <div><i class="fas fa-user-alt me-2"></i>Işgärler</div>
                <div class="button-add">
                    <a href="/kadr/categories" class="btn btn-success"><i class="bi bi-bars"></i> Bölümler</a>
                    <a href="/kadr/user-add" class="btn btn-primary"><i class="bi bi-plus"></i> Goşmak</a>
                </div>
            </div>
              <h5 class="card-title"></h5>
              <p></p>

              <table class="table datatable">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Familýasy</th>
                        <th>Ady</th>
                        <th>Atasynyň ady</th>
                        <th>Tel No</th>
                        <th>Doglan senesi</th>
                        <th>Wezipesi</th>
                        <th>Ýaşaýan ýeri</th>
                        <th>Suraty</th>
                        <th>Sazlamalar</th>
                    </tr>
                </thead>
                <tbody>
                    <% bloglar.forEach(user=> { %>
                        <tr>
                            <td>
                                <%= user.id %>
                            </td>
                            <td>
                                <%= user.surname %>
                            </td>
                            <td>
                                <%= user.name %>
                            </td>
                            <td>
                                <%= user.ata_name %>
                            </td>
                            <td>
                                <%= user.tel_nom %>
                            </td>
                            <td>
                                <%= user.birth %>
                            </td>
                            <td>
                                <%= user.duty %>
                            </td>
                            <td>
                                <%= user.address %>
                            </td>
                            <td>
                                <img src="/uploads/user/<%= user.user_img %>" width="150"
                                    height="100" alt="tapylmady">
                            </td>
                            <td>
                                <a href="/kadr/user/<%= user.id %>" class="btn btn-primary"> <i
                                        class="bi bi-eye"></i></a>
                                <a href="/kadr/user/delete/<%= user.id %>"
                                    class="btn btn-danger"><i class="bi bi-trash"></i></a>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
              </table>
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->





  <!-- Vendor JS Files -->
  <script src="/admin/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/admin/assets/vendor/quill/quill.js"></script>
  <script src="/admin/assets/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="/admin/assets/vendor/tinymce/tinymce.min.js"></script>

  <!-- Template Main JS File -->
  <script src="/admin/assets/js/main.js"></script>

</body>

</html>